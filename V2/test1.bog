fun my_func(x, r)
{
	if(r)
	{
		return x*x + 100.0;
	}
	else
	{
		return x + exp(x);
	};
}

;;; This is my_func differentiated with respect to x
fun d_my_func(x, r)
{
	var myvar, mycar, mysar;
	
	myvar = 10;
	mycar = myvar*(myvar+myvar);
	mysar = 100 - mycar;
	
	return x+r;
}

